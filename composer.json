{
    "name": "coderello/laraflash",
    "description": "Advanced flash messages for Laravel.",
    "keywords": ["laraflash", "laravel", "advanced", "flash", "messages"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "laravel/framework": "5.5.*|5.6.*|5.7.*"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Coderello\\Laraflash\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Coderello\\Laraflash\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Coderello\\Laraflash\\Providers\\LaraflashServiceProvider"
            ]
        }
    }
}
